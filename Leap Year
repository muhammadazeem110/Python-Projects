# Taking input from the user
year = int(input("Enter the year to find weather it's leap year or not : "))
if year % 4 == 0:
    if year % 100 != 0:
        print(f"{year} is the leap year.")
    else:
        if year % 400 == 0:
            print(f"{year} is the leap year.")
        else:
            print(f"{year} is not leap year.")
else:
    print(f"{year} is not leap year.")
